{
  "name": "wordpress-theme-webpack",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run clean && export NODE_ENV=watch && webpack-dev-server --progress --colors --watch --config webpack.config.dev.js",
    "clean": "rm -rf public",
    "clean:all": "rm -rf public node_modules",
    "build:dev": "npm run clean && webpack --config webpack.config.dev.js",
    "build:prod": "npm run clean && webpack --config webpack.config.prod.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.7.2",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "chokidar": "^2.1.8",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^7.0.2",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.75.0",
    "webpack-build-notifier": "^1.2.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "jquery.scrollex": "^0.2.1",
    "rellax": "^1.10.0"
  }
}
